config CPU_FAM_RP2350_RISCV
    bool
    select CPU_CORE_RV32IMAC

config CPU_FAM
    default "RP2350_RISCV" if CPU_FAM_RP2350_RISCV

config CPU_MODEL_RP2350_RISCV
    bool
    select CPU_FAM_RP2350_RISCV

config CPU_MODEL
    default "RP2350_RISCV" if CPU_MODEL_RP2350_RISCV

config CPU
    default "rp2350_riscv" if CPU_FAM_RP2350_RISCV


source "$(RIOTCPU)/riscv_common/Kconfig"
